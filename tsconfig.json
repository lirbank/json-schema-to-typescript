{
  "compilerOptions": {
    "declaration": false,
    "lib": ["es2015", "esnext", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "rootDir": "src"
  },
  "exclude": [
    "example",
    "node_modules",
    "test"
  ]
}
